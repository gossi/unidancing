{
  "name": "@unidancing/gatekeeper",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy --env production --outdir ./dist && pnpm run sentry:sourcemaps",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org gossi --project unidancing-gatekeeper ./dist && sentry-cli sourcemaps upload --org gossi --project unidancing-gatekeeper ./dist",
    "lint": "pnpm run '/^lint:(?!.*(fix)).*$/'",
    "lint:fix": "pnpm run '/^lint:[^:]*:fix/'",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix"
  },
  "dependencies": {
    "hono": "^4.6.19",
    "@hono/sentry": "^1.2.0",
    "oauth4webapi": "^3.1.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250129.0",
    "@eslint/compat": "^2.0.0",
    "@gossi/config-eslint": "^1.3.1",
    "@gossi/config-prettier": "^1.1.1",
    "@sentry/cli": "^2.41.1",
    "eslint": "^9.39.1",
    "prettier": "^3.6.2",
    "typescript": "^5.7.3",
    "wrangler": "^3.107.2"
  },
  "engines": {
    "node": ">=22.16"
  }
}
